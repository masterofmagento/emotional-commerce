      <script type="text/javascript">
        require(['jquery', 'jquery/ui'], function($) {
          $(document).ready(function($) {
            $("#button-background-text").on("change paste keyup", function() {
              $('#button-title').text($(this).val());
            });
            $("#button-background").on("change paste keyup", function() {
              $('#qr-modal-btn').css("background-color", $(this).val());                
              $('#gift-box-svg5').css({ fill: $(this).val()});
            });            

            $("#button-color").on("change paste keyup", function() {
              $('#qr-modal-btn').css("color", $(this).val());
              $('#gift-box-svg1').css({ fill: $(this).val()});
              $('#gift-box-svg2').css({ fill: $(this).val()});
              $('#gift-box-svg3').css({ fill: $(this).val()});
              $('#gift-box-svg4').css({ fill: $(this).val()});               
            });

            /*Remove banner code start*/                     
            if($("#dynamic_banner").hasClass("image-uplode-privew1")){
              $(".remove-banner-button").hide();
            }else{            
              $(".remove-banner-button").show();
            }

            $('button.copyButton').click(function(){
              $(this).siblings('input.linkToCopy').select();                
              $(".copyButton" ).html("Copied!");      
              document.execCommand("copy");
            });          

            var prevVal = $(".main-qr-button-wrapper").attr('class').split(' ')[1];
            $("#dynamic-button-preview").on("change",function(){
              var val = $(this).find('option:selected').val();
              $(".main-qr-button-wrapper").removeClass(prevVal).addClass(val);
              $(".image-uplode-privew").removeClass(prevVal).addClass(val);
              prevVal = val;             
            });
          });
        });
        function openTab(evt, TabName) {
          var i, tabcontent, tablinks;
          tabcontent = document.getElementsByClassName("dashboardbody__tabcontent");
          for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = "none";
          }
          tablinks = document.getElementsByClassName("TabLinks");
          for (i = 0; i < tablinks.length; i++) {
            tablinks[i].className = tablinks[i].className.replace(" active", "");
          }
          document.getElementById(TabName).style.display = "flex";
          evt.currentTarget.className += " active";
        }
      </script>
